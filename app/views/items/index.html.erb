      <th>Name</th>
      <th>Type</th>
      <th>Mintemperature</th>
      <th>Maxtemperature</th>
      <th>Season</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
