<div class="user--show">
  <div class="row">
    <aside class="col s12 m3 l3">
      <div class="col s3 m12 l12">  <%=  gravatar_for(@user) %></div>
      <div class="col s9 m12 l12"><h5> <%= @user.first_name %> <%= @user.last_name%></h5> 
       <%= @user.email %>
       <br/><%= @user.bio %> </div>
    </aside>

 <%= render template: "lists/index"%>
 
  </div>
  
  </div>
</div>

<% content_for :script  do%> <!-- type JS code here to add into js at the footer -->
function initCollapse()
{
  $('.collapsible').collapsible({
    accordion : false // A setting that changes the collapsible behavior to expandable instead of the default accordion style
  });
}

$(document).ajaxComplete(initCollapse);
<% end %>