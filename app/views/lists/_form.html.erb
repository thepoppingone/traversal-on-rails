<%= form_for @list, remote: true do |f|   %>
  <%= f.label  :name                      %>
  <%= f.text_field  :name                      %>
  <%= f.label  :description %>
  <%= f.text_field  :description                     %>
  <%= f.hidden_field :user_id, :value => @user.id %>
<%= f.submit "Create" %>
<% end %>
